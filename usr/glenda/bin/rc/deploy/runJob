#!/bin/rc
#host=9.3.45.5
host=127.1
port=5670
cmd='date'
input=cmdlist
#srv -b tcp!$host!$port csrv
mount /srv/csrv /n/XCPU3
#echo debug 0 > /n/XCPU3/local/ctl
@{
x=`{cat }
echo $x
cat /n/XCPU3/local/$x/status
rsrc=`{cat /n/XCPU3/local/status}
echo $rsrc
echo res 1 >/n/XCPU3/local/$x/ctl
#cat /n/XCPU3/local/$x/stdio
echo exec $cmd >/n/XCPU3/local/$x/ctl
cat /n/XCPU3/local/$x/stdio 
}<>/n/XCPU3/local/clone 

unmount /n/XCPU3

