#!/bin/rc
srv -b tcp!9.3.45.5!5544 csrv
mount /srv/csrv /n/127.1!5544
@{
x=`{cat }
echo $x
cat /n/127.1!5544/csrv/local/$x/status
#rsrc=`{cat /n/127.1!5544/csrv/local/status}
#echo $rsrc
echo res 1 >/n/127.1!5544/csrv/local/$x/ctl
#cat /n/127.1!5544/csrv/local/$x/status
echo exec pwd >/n/127.1!5544/csrv/local/$x/ctl
cat /n/127.1!5544/csrv/local/$x/stdio 
#cat /n/127.1!5544/csrv/local/$x/status
#cat /n/127.1!5544/csrv/local/$x/status
}<>/n/127.1!5544/csrv/local/clone 

unmount /n/127.1!5544

