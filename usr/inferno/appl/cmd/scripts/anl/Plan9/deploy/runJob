#!/bin/rc
#host=9.3.45.5
host=127.1
port=5544
srv -b tcp!$host!$port csrv
mount /srv/csrv /n/XCPU3
@{
x=`{cat }
echo $x
cat /n/XCPU3/csrv/local/$x/status
rsrc=`{cat /n/XCPU3/csrv/local/status}
echo $rsrc
echo res 1 >/n/XCPU3/csrv/local/$x/ctl
cat /n/XCPU3/csrv/local/$x/status
echo exec echo test >/n/XCPU3/csrv/local/$x/ctl
cat /n/XCPU3/csrv/local/$x/stdio 
cat /n/XCPU3/csrv/local/$x/status
cat /n/XCPU3/csrv/local/$x/status
}<>/n/XCPU3/csrv/local/clone 

unmount /n/XCPU3

