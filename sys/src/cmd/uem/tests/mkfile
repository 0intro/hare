</$objtype/mkfile

TARG = \
	mp-writer\

BIN = /$objtype/bin
BIN = /usr/$user/bin/$objtype

</sys/src/cmd/mkmany


regress:V: install
	rc mpipefs.rc
	rc execfs.rc
	rc gangfs.rc

debug:V: install
	rc mpipefs.rc 9
	rc execfs.rc 9
	rc gangfs.rc 9

tidy:VE:
	rm -f /srv/mpipe
	kill execcmd | rc
	kill gangfs | rc
	kill execfs | rc
	kill mpipefs | rc
	broke | rc
	rm -f *.log
	rm -f *.acid
   	rm -rf .test
	
	unmount /proc
	bind '#p' /proc

acid:V: execfs.acid execcmd.acid mpipefs.acid gangfs.acid
